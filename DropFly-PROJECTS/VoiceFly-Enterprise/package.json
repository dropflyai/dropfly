{
  "name": "@voicefly/enterprise",
  "version": "1.0.0",
  "description": "VoiceFly Enterprise - AI Voice Agents with Real-time Web Intelligence",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "deploy:staging": "./scripts/deploy/staging.sh",
    "deploy:production": "./scripts/deploy/production.sh",
    "backup": "./scripts/maintenance/backup.sh",
    "logs": "./scripts/maintenance/logs.sh",
    "health-check": "./scripts/maintenance/health-check.sh",
    "version:patch": "./scripts/version/patch.sh",
    "version:minor": "./scripts/version/minor.sh",
    "version:major": "./scripts/version/major.sh",
    "rollback": "./scripts/deploy/rollback.sh",
    "monitor": "./scripts/monitoring/start.sh"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "semantic-release": "^22.0.0"
  },
  "dependencies": {
    "dotenv": "^16.3.0",
    "zod": "^3.22.0"
  },
  "keywords": [
    "voice-ai",
    "web-intelligence",
    "sales-automation", 
    "b2b-sales",
    "vapi",
    "enterprise"
  ],
  "author": "DropFly Technologies",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dropfly/voicefly-enterprise.git"
  },
  "homepage": "https://voicefly.ai"
}